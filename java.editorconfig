# java_resource_list_wrap = off|normal|on_every_item|split_into_lines
# java_space_after_closing_angle_bracket_in_type_argument = true|false
# java_space_before_type_parameter_list = true|false
# java_spaces_around_lambda_arrow = true|false
# java_spaces_around_method_ref_dbl_colon = true|false
# java_spaces_around_type_bounds_in_type_parameters = true|false
# java_spaces_within_angle_brackets = true|false
# java_spaces_within_annotation_parentheses = true|false
# java_spaces_within_synchronized_parentheses = true|false
