# Java-specific EditorConfig properties

[*.java]

# java_doc_add_blank_after_description = true|false
# java_doc_add_blank_after_param_comments = true|false
# java_doc_add_blank_after_return = true|false
# java_doc_align_param_comments = true|false
# java_doc_do_not_wrap_one_line_comments = true|false
# java_doc_keep_empty_lines = true|false
# java_doc_keep_empty_parameter = true|false
# java_doc_keep_empty_return = true|false
# java_doc_keep_invalid_tags = true|false
# java_doc_param_description_on_new_line = true|false
